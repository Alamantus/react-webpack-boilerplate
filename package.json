{
  "name": "react-webpack-boilerplate",
  "title": "React+Webpack Boilerplate",
  "version": "0.1.0",
  "description": "A simple boilerplate that does everything I need to get a ReactJS project up and running with Webpack.",
  "main": "src/index.jsx",
  "scripts": {
    "start": "node start-server.js",
    "serve": "node start-server.js",
    "pack": "node ./node_modules/webpack/bin/webpack.js -d --progress --display-error-details",
    "watch": "node ./node_modules/webpack/bin/webpack.js -d --progress --watch",
    "build": "node ./node_modules/webpack/bin/webpack.js -p --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Alamantus@github.com/Alamantus/react-webpack-boilerplate.git"
  },
  "author": "Robbie Antenesse <gamedev@alamantus.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alamantus/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/Alamantus/react-webpack-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "bulma": "^0.2.3",
    "img-loader": "^1.3.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-minify-loader": "^1.1.0",
    "node-sass": "^3.10.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  }
}
